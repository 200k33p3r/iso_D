
&iso_controls

   do_isochrones = .true.

   do_smooth = .true.

   do_PAV = .true.

   do_colors = .true.
   BC_table_list = 'bc_table.list'

   do_Cstars = .true.
   cstar_table_list = 'cstar_table.list'

   cmd_suffix = 'cmd'

   extinction_Av = 0.0

